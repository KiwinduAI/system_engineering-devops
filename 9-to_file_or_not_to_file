chmod +x 9-to_file_or_not_to_file

# Test cases
rm -rf school         # Ensure 'school' does not exist
./9-to_file_or_not_to_file

touch school          # Create empty file
./9-to_file_or_not_to_file

echo "betty" > school # Add content
./9-to_file_or_not_to_file

rm school
mkdir school          # Make directory
./9-to_file_or_not_to_file
